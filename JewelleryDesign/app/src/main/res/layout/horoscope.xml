<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >
 <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fadingEdge="none"
                android:fillViewport="true"
                android:scrollbars="none" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/dddd"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/logo"
            android:layout_marginBottom="10dp"
            android:orientation="vertical" >
<TextView
                android:id="@+id/txtUsejgghjjrname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="HOROSCOPE"
                 android:layout_marginTop="10dp"
                android:textColor="#000000"
                android:textSize="15sp" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/backgroundss"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:ellipsize="start"
                    android:hint="Full Name"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:padding="15dp"
                    android:textColor="@color/ratnajyoti"
                    android:textColorHint="@color/black"
                    android:textSize="13sp" >

                    <requestFocus />
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/backgroundss"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/mobile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:ellipsize="start"
                    android:hint="Contact No"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:padding="15dp"
                    android:textColor="@color/ratnajyoti"
                    android:textColorHint="@color/black"
                    android:textSize="13sp" >

                    <requestFocus />
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/backgroundss"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:ellipsize="start"
                    android:hint="Email Address"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:padding="15dp"
                    android:textColor="@color/ratnajyoti"
                    android:textColorHint="@color/black"
                    android:textSize="13sp" >

                    <requestFocus />
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/backgroundss"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/tellabout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:ellipsize="start"
                    android:hint="DOB dd/mm/yyyy"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:lines="2"
                    android:padding="15dp"
                    android:textColor="@color/ratnajyoti"
                    android:textColorHint="@color/black"
                    android:textSize="13sp" >

                    <requestFocus />
                </EditText>
            </LinearLayout>

            
             <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/backgroundss"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/tellajjjbout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:ellipsize="start"
                    android:hint="Time Of Birth hh:mm"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:lines="2"
                    android:padding="15dp"
                    android:textColor="@color/ratnajyoti"
                    android:textColorHint="@color/black"
                    android:textSize="13sp" >

                    <requestFocus />
                </EditText>
            </LinearLayout>
            
                 
                      <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="5dp"
        
        android:layout_below="@+id/lin1" >

 <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:orientation="horizontal" >
                     <TextView
                android:id="@+id/txtUsername"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Gender"
                android:textColor="#000000"
                android:textSize="15sp" />
        <RadioButton
            android:id="@+id/radio0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:text="male" />
          <RadioButton
            android:id="@+id/radio1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:text="Female" />
          </LinearLayout>
                    
                  </RadioGroup>    
                    
             
                <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/backgroundss"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/tellajhhjjbout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:ellipsize="start"
                    android:hint="Place of Birth"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:lines="2"
                    android:padding="15dp"
                    android:textColor="@color/ratnajyoti"
                    android:textColorHint="@color/black"
                    android:textSize="13sp" >

                    <requestFocus />
                </EditText>
            </LinearLayout>
             
                
                
                
                     <RadioGroup
        android:id="@+id/radioGroup1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="5dp"
        
        android:layout_below="@+id/lin1" >

 <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:orientation="horizontal" >
                     <TextView
                android:id="@+id/txtUsehhrname"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Language"
                android:textColor="#000000"
                android:textSize="15sp" />
        <RadioButton
            android:id="@+id/radio00"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:text="English" />
          <RadioButton
            android:id="@+id/radio11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:text="Hindi" />
          </LinearLayout>
                    
                  </RadioGroup>    
                    
                
            <Spinner
                android:id="@+id/spinner"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/spinner_background"
                android:spinnerMode="dropdown" >
            </Spinner>

               <RadioGroup
        android:id="@+id/radioGroup2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="5dp"
        
        android:layout_below="@+id/lin1" >

 
                     <TextView
                android:id="@+id/txtUsehhhhhrname"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="20dp"
                android:text="Horoscope Charge"
                android:textColor="#000000"
                android:textSize="15sp" />
        <RadioButton
            android:id="@+id/radio01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:layout_marginLeft="20dp"
            android:text=" 501 INR [7.48 USD]" />
          <RadioButton
            android:id="@+id/radio12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:layout_marginLeft="20dp"
            android:text="1001 INR [14.95 USD]" />
          
                    
                  </RadioGroup>    

            <Button
                android:id="@+id/submit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/button_borders"
                android:padding="5dp"
                android:text="Submit"
                android:textColor="#ffffff"
                android:textSize="13sp" />
        </LinearLayout>
    </LinearLayout>
    
</ScrollView>
</RelativeLayout>
